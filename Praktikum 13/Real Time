import pandas as pd

df = pd.read_csv("C:/Lab/algo/negara.csv")

mean = df.groupby(['Benua']).mean()
std = df.groupby(['Benua']).std()

print(df)
print('\nBerikut Data Mean')
print(mean)
print('\nBerikut Data Standard Deviation')
print(std) 
